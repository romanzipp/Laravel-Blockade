{
    "name": "romanzipp/laravel-blockade",
    "description": "Laravel Blockade package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "ext-json": "*",
        "illuminate/console": "^5.5|^6.20.12|^7.30.3|^8.22.1",
        "illuminate/support": "^5.5|^6.20.12|^7.30.3|^8.22.1",
        "spatie/url": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "laravel/framework": "^5.5|^6.20.12|^7.30.3|^8.22.1",
        "mockery/mockery": "^1.3.2",
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "romanzipp/php-cs-fixer-config": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\Blockade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\Blockade\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\Blockade\\Providers\\BlockadeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
